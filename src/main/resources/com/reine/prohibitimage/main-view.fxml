<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.reine.prohibitimage.fxcontroller.MainController">
    <children>
        <StackPane prefHeight="341.0" prefWidth="600.0">
            <children>
                <ImageView fx:id="imageView" fitHeight="256.0" fitWidth="377.0" pickOnBounds="true"
                           preserveRatio="true"/>
            </children>
        </StackPane>
        <HBox alignment="CENTER" prefHeight="61.0" prefWidth="600.0" spacing="50.0">
            <children>
                <Button mnemonicParsing="false" onAction="#chooseImage" text="选择图片"/>
                <Button mnemonicParsing="false" onAction="#generateImage" text="生成图片"/>
                <Button mnemonicParsing="false" onAction="#copyImage" text="复制图片"/>
                <Button mnemonicParsing="false" onAction="#saveImage" text="保存图片"/>
            </children>
            <VBox.margin>
                <Insets/>
            </VBox.margin>
        </HBox>
    </children>
</VBox>
